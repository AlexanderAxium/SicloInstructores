    "bcryptjs": "^3.0.2",
    "better-auth": "^1.3.23",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "lucide-react": "^0.544.0",
    "next": "^15.2.3",
    "next-intl": "^4.3.12",
    "next-themes": "^0.4.6",
