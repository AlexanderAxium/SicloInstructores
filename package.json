{
  "name": "modern-app-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "biome check .",
    "commit": "cz",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "check:unsafe": "biome check --write --unsafe .",
    "check:write": "biome check --write .",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:update": "npm-run-all db:reset db:seed",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose stop",
    "db:down": "docker-compose down",
    "db:clean": "docker-compose down -v",
    "db:logs": "docker-compose logs -f",
    "dev": "npm-run-all db:start dev:wait dev:push dev:next",
    "dev:wait": "wait-on tcp:5432 -t 10000",
    "dev:push": "prisma db push",
    "dev:next": "next dev --turbo",
    "postinstall": "prisma generate",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-accordion": "^1.2.12",
